{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "OrchestratorAgentOutputs",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "agent_queue": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "agent_id": {"type": "string"},
          "task": {"type": "string"},
          "inputs": {"type": "object"}
        },
        "required": ["agent_id", "task"]
      }
    },
    "delegation_map": {"type": "object"},
    "priority_order": {"type": "array", "items": {"type": "string"}}
  },
  "required": ["agent_queue", "delegation_map"]
}
